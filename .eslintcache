p/src/Components/TodoList.tsx",[],"/Users/oem/Desktop/todoapp/src/Components/AddTodoForm.tsx",["171"],"import React from 'react';\nimport { useTodoActions, useLabelState } from '../Hooks';\nimport 'react-datepicker/dist/react-datepicker.css';\nimport { Formik, Form } from 'formik';\nimport * as yup from 'yup';\nimport DatePicker from '../Wrappers/DatePicker';\nimport styled from 'styled-components';\n\nconst AddTodoInput = styled.input`\n\theight: 30px;\n\tfont-size: 15px;\n\toutline: none;\n\tborder-radius: 5px;\n\tborder: 1px solid black;\n\tpadding: 5px;\n\twidth: 100%;\n`;\nconst Error = styled.div`\n\tcolor: red;\n`;\nconst SelectTodoItem = styled.select`\n\theight: 30px;\n\toutline: none;\n\tborder: 1px solid black;\n\tborder-radius: 5px;\n\tmargin-top: 10px;\n`;\nconst SaveButton = styled.button`\n\tbackground: tomato;\n\tborder: 1px solid tomato;\n\toutline: none;\n\tpadding: 8px;\n\tborder-radius: 5px;\n\tcolor: white;\n\tfont-weight: 550;\n\tcursor: pointer;\n`;\nconst CancelButton = styled.button`\n\tbackground: transparent;\n\toutline: none;\n\tpadding: 8px;\n\tborder-radius: 5px;\n\tfont-weight: 550;\n\tmargin-left: 5px;\n\tborder: 1px solid #eeeeee;\n\tcursor: pointer;\n`;\n\ninterface IProps {\n\thandleCloseAddTodoForm: () => void;\n}\n\nexport const AddTodoForm: React.FC<IProps> = ({ handleCloseAddTodoForm }) => {\n\tconst { addTodo } = useTodoActions();\n\tconst { selectedLabel } = useLabelState();\n\n\tconst handleSubmit = React.useCallback(\n\t\t(todo: Omit<Todo, 'id'>) => {\n\t\t\taddTodo({ ...todo, categoryId: selectedLabel?.id });\n\t\t\thandleCloseAddTodoForm();\n\t\t},\n\t\t[selectedLabel],\n\t);\n\n\tconst validationSchema = yup.object({\n\t\ttext: yup.string().required('This field cannot stay empty...!!!'),\n\t});\n\n\treturn (\n\t\t<Formik\n\t\t\tinitialValues={{\n\t\t\t\ttext: '',\n\t\t\t\tpriority: 'Low',\n\t\t\t\tdate: new Date(),\n\t\t\t}}\n\t\t\tvalidationSchema={validationSchema}\n\t\t\tvalidateOnChange={true}\n\t\t\tonSubmit={handleSubmit}>\n\t\t\t{({ errors, touched, handleChange, setFieldValue, values }) => (\n\t\t\t\t<div>\n\t\t\t\t\t<Form>\n\t\t\t\t\t\t<div style={{ width: '98%', marginTop: 10, marginLeft: 5 }}>\n\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t\t<AddTodoInput\n\t\t\t\t\t\t\t\t\t\ttype='text'\n\t\t\t\t\t\t\t\t\t\tname='text'\n\t\t\t\t\t\t\t\t\t\tplaceholder='Add task'\n\t\t\t\t\t\t\t\t\t\tonChange={handleChange}\n\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t{errors.text && touched.text ? (\n\t\t\t\t\t\t\t\t\t\t<Error>{errors.text}</Error>\n\t\t\t\t\t\t\t\t\t) : null}\n\t\t\t\t\t\t\t\t\t<div style={{ display: 'flex' }}>\n\t\t\t\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t\t\t\t<SelectTodoItem onChange={handleChange}>\n\t\t\t\t\t\t\t\t\t\t\t\t<option>Low</option>\n\t\t\t\t\t\t\t\t\t\t\t\t<option>Medium</option>\n\t\t\t\t\t\t\t\t\t\t\t\t<option>High</option>\n\t\t\t\t\t\t\t\t\t\t\t</SelectTodoItem>\n\t\t\t\t\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t\t\t\t\t<div style={{ marginTop: 10 }}>\n\t\t\t\t\t\t\t\t\t\t\t<DatePicker\n\t\t\t\t\t\t\t\t\t\t\t\tselected={values.date}\n\t\t\t\t\t\t\t\t\t\t\t\tonChange={(date: any) => setFieldValue('date', date)}\n\t\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div style={{ paddingTop: 15 }}>\n\t\t\t\t\t\t\t\t\t<SaveButton type='submit'>Save</SaveButton>\n\t\t\t\t\t\t\t\t\t<CancelButton onClick={handleCloseAddTodoForm}>\n\t\t\t\t\t\t\t\t\t\tCancel\n\t\t\t\t\t\t\t\t\t</CancelButton>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</Form>\n\t\t\t\t</div>\n\t\t\t)}\n\t\t</Formik>\n\t);\n};\n\n// TODO make submit from button on onCLick\n","/Users/oem/Desktop/todoapp/src/Components/TodoListItem.tsx",[],"/Users/oem/Desktop/todoapp/src/Components/StyledComponents.tsx",["172"],"import styled from 'styled-components';\n",["173","174"],"/Users/oem/Desktop/todoapp/src/api/index.ts",["175"],"import { getLsItem, updateLsItem } from '../services/localStorage';\n\n\nconst TODOS_LS_KEY = 'TODOS_LS_KEY'\n\nexport function getTodos(): Promise<Todo[]> {\n    const todos = getLsItem(TODOS_LS_KEY, [])\n    return Promise.resolve(todos)\n}\n\nexport function createTodo(todo: Omit<Todo, 'id'>): Promise<Todo> {\n    const newTodo = {\n        ...todo,\n        id: Date.now()\n    }\n    updateLsItem(TODOS_LS_KEY, (current) => {\n            return [...current, newTodo]\n    }, [])\n    return Promise.resolve(newTodo)\n}\nexport function removeTodo(id: number): Promise<number> {\n    updateLsItem(TODOS_LS_KEY, (current) => {\n        return current.filter((todo: Todo) => todo.id !== id)\n    }, [])\n    return Promise.resolve(id)\n}\nexport function completeTodo(selectedTodo: Todo): Promise<Todo> {\n    updateLsItem(TODOS_LS_KEY, (current) => {\n        return current.map((todo: Todo) => {\n\t\t\tif (todo.id === selectedTodo.id) {\n\t\t\t\treturn {\n\t\t\t\t\t...todo,\n\t\t\t\t\tcomplete: !todo.complete,\n\t\t\t\t};\n\t\t\t}\n\t\t})\n    }, [])\n    return Promise.resolve(selectedTodo)\n}\n\nexport function editTodo(todo: Todo): Promise<Todo> {\n\n    updateLsItem(TODOS_LS_KEY, (current) => {\n        return current.map((i: Todo) => i.id === todo.id ? todo : i)\n    }, [])\n    return Promise.resolve(todo)\n}",["176","177"],"/Users/oem/Desktop/todoapp/src/Api/index.ts",[],"/Users/oem/Desktop/todoapp/src/Components/LabelList.tsx",[],"/Users/oem/Desktop/todoapp/src/Providers/ModalProvider.tsx",[],"/Users/oem/Desktop/todoapp/src/Icon/Icon.tsx",[],"/Users/oem/Desktop/todoapp/src/Components/AddLabelForm.tsx",["178"],"import { Formik, Form } from 'formik';\nimport * as yup from 'yup';\nimport styled from 'styled-components';\nimport { useLabelActions } from '../Hooks';\nimport { useModal } from '../Providers/ModalProvider';\nimport Switch from './Switch';\n\nconst Input = styled.input`\n\twidth: 95%;\n\theight: 30px;\n\tfont-size: 15px;\n\toutline: none;\n\tborder-radius: 5px;\n\tborder: 1px solid black;\n\tpadding: 5px;\n`;\nconst Error = styled.div`\n\tcolor: tomato;\n`;\n\nconst ButtonContainer = styled.div`\n\tpadding-top: 15px;\n`;\n\nconst SaveButton = styled.button`\n\tbackground: tomato;\n\tborder: 1px solid tomato;\n\toutline: none;\n\tpadding: 8px;\n\tborder-radius: 5px;\n\tcolor: white;\n\tfont-weight: 550;\n\tcursor: pointer;\n`;\nconst CancelButton = styled.button`\n\tbackground: #f5f5f5;\n\toutline: none;\n\tpadding: 8px;\n\tborder-radius: 5px;\n\tfont-weight: 550;\n\tmargin-left: 5px;\n\tborder: 1px solid #eeeeee;\n\tcursor: pointer;\n`;\n\nconst validationSchema = yup.object({\n\ttitle: yup\n\t\t.string()\n\t\t.min(2, 'Too Short!')\n\t\t.max(20, 'Too Long!')\n\t\t.required('Required'),\n});\n\nexport const AddLabelForm = () => {\n\tconst { addLabel } = useLabelActions();\n\tconst { closeModalDialog } = useModal();\n\n\treturn (\n\t\t<div>\n\t\t\t<Formik\n\t\t\t\tinitialValues={{ title: '' }}\n\t\t\t\tvalidationSchema={validationSchema}\n\t\t\t\tvalidateOnChange={true}\n\t\t\t\tonSubmit={(i) => {\n\t\t\t\t\taddLabel({ title: i.title });\n\t\t\t\t\tcloseModalDialog();\n\t\t\t\t}}>\n\t\t\t\t{({ errors, touched, handleChange }) => (\n\t\t\t\t\t<Form>\n\t\t\t\t\t\t<h3 style={{ marginLeft: 5, margin: 3, padding: 3 }}>Add Label</h3>\n\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t<Input\n\t\t\t\t\t\t\t\tname='title'\n\t\t\t\t\t\t\t\tplaceholder='Add label'\n\t\t\t\t\t\t\t\tonChange={handleChange}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t{errors.title && touched.title ? (\n\t\t\t\t\t\t\t\t<Error>{errors.title}</Error>\n\t\t\t\t\t\t\t) : null}\n\t\t\t\t\t\t\t<ButtonContainer>\n\t\t\t\t\t\t\t\t<SaveButton type='submit'>SAVE</SaveButton>\n\t\t\t\t\t\t\t\t<CancelButton onClick={closeModalDialog}>CANCEL</CancelButton>\n\t\t\t\t\t\t\t</ButtonContainer>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</Form>\n\t\t\t\t)}\n\t\t\t</Formik>\n\t\t</div>\n\t);\n};\n","/Users/oem/Desktop/todoapp/src/Components/LabelItem.tsx",[],"/Users/oem/Desktop/todoapp/src/Modal/Modal.tsx",[],"/Users/oem/Desktop/todoapp/src/shared/localStorage.ts",[],"/Users/oem