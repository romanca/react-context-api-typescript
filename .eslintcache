e='text'\n\t\t\t\t\t\t\t\t\t\tname='text'\n\t\t\t\t\t\t\t\t\t\tplaceholder='Add task'\n\t\t\t\t\t\t\t\t\t\tonChange={handleChange}\n\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t{errors.text && touched.text ? (\n\t\t\t\t\t\t\t\t\t\t<Error>{errors.text}</Error>\n\t\t\t\t\t\t\t\t\t) : null}\n\t\t\t\t\t\t\t\t\t<div style={{ display: 'flex' }}>\n\t\t\t\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t\t\t\t<SelectTodoItem onChange={handleChange}>\n\t\t\t\t\t\t\t\t\t\t\t\t<option>Low</option>\n\t\t\t\t\t\t\t\t\t\t\t\t<option>Medium</option>\n\t\t\t\t\t\t\t\t\t\t\t\t<option>High</option>\n\t\t\t\t\t\t\t\t\t\t\t</SelectTodoItem>\n\t\t\t\t\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t\t\t\t\t<div style={{ marginTop: 10 }}>\n\t\t\t\t\t\t\t\t\t\t\t<DatePicker\n\t\t\t\t\t\t\t\t\t\t\t\tselected={values.date}\n\t\t\t\t\t\t\t\t\t\t\t\tonChange={(date: any) => setFieldValue('date', date)}\n\t\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div style={{ paddingTop: 15 }}>\n\t\t\t\t\t\t\t\t\t<SaveButton type='submit'>Save</SaveButton>\n\t\t\t\t\t\t\t\t\t<CancelButton onClick={handleCloseAddTodoForm}>\n\t\t\t\t\t\t\t\t\t\tCancel\n\t\t\t\t\t\t\t\t\t</CancelButton>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</Form>\n\t\t\t\t</div>\n\t\t\t)}\n\t\t</Formik>\n\t);\n};\n\n// TODO make submit from button on onCLick\n","/Users/oem/Desktop/todoapp/src/Components/TodoListItem.tsx",[],"/Users/oem/Desktop/todoapp/src/Components/StyledComponents.tsx",["168"],"import styled from 'styled-components';\n",["169","170"],"/Users/oem/Desktop/todoapp/src/api/index.ts",["171"],"import { getLsItem, updateLsItem } from '../services/localStorage';\n\n\nconst TODOS_LS_KEY = 'TODOS_LS_KEY'\n\nexport function getTodos(): Promise<Todo[]> {\n    const todos = getLsItem(TODOS_LS_KEY, [])\n    return Promise.resolve(todos)\n}\n\nexport function createTodo(todo: Omit<Todo, 'id'>): Promise<Todo> {\n    const newTodo = {\n        ...todo,\n      